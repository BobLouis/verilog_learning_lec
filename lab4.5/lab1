#! /usr/local/Cellar/icarus-verilog/11.0/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/local/Cellar/icarus-verilog/11.0/lib/ivl/system.vpi";
:vpi_module "/usr/local/Cellar/icarus-verilog/11.0/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/Cellar/icarus-verilog/11.0/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/Cellar/icarus-verilog/11.0/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/Cellar/icarus-verilog/11.0/lib/ivl/va_math.vpi";
S_0x7fb3f5f05f40 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v0x7fb3f5f18c40_0 .var "in", 3 0;
v0x7fb3f5f18d00_0 .net "out", 3 0, v0x7fb3f5f18b80_0;  1 drivers
S_0x7fb3f5f060b0 .scope module, "u1" "shifter" 2 7, 2 53 0, S_0x7fb3f5f05f40;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "in";
    .port_info 1 /OUTPUT 4 "out";
v0x7fb3f5f04630_0 .net "in", 3 0, v0x7fb3f5f18c40_0;  1 drivers
v0x7fb3f5f18b80_0 .var "out", 3 0;
E_0x7fb3f5f08440 .event edge, v0x7fb3f5f04630_0;
    .scope S_0x7fb3f5f060b0;
T_0 ;
    %wait E_0x7fb3f5f08440;
    %load/vec4 v0x7fb3f5f04630_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %store/vec4 v0x7fb3f5f18b80_0, 0, 4;
    %pushi/vec4 1, 0, 1;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x7fb3f5f18b80_0, 4, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x7fb3f5f05f40;
T_1 ;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x7fb3f5f18c40_0, 0, 4;
    %delay 5, 0;
    %load/vec4 v0x7fb3f5f18d00_0;
    %cmpi/e 13, 0, 4;
    %jmp/0xz  T_1.0, 4;
    %vpi_call 2 23 "$display", "in = %b; The result of arithmetically shift right = %b", v0x7fb3f5f18c40_0, v0x7fb3f5f18d00_0 {0 0 0};
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x7fb3f5f18d00_0;
    %cmpi/e 5, 0, 4;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 2 27 "$display", "in = %b; The result of logically shift right = %b", v0x7fb3f5f18c40_0, v0x7fb3f5f18d00_0 {0 0 0};
    %jmp T_1.3;
T_1.2 ;
    %vpi_call 2 31 "$display", "The input does not shift correctly!" {0 0 0};
T_1.3 ;
T_1.1 ;
    %delay 20, 0;
    %vpi_call 2 39 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "lab1.v";
